    ORR     r1, r2, r3, LSR #7
    ORR     r1, r2, r3, ASR #7
    ORR     r1, r2, r3, ROR #7
    ORR     r1, r2, r3, RRX

Test: Logical (constant)
    ORR     r1, r2, #1
    ORR     r1, r2, #&248             ; #584
    ORR     r1, r2, #&9900            ; #39168 = 153<<8
    ORR     r1, r2, #&88000           ; #557056 = 136<<12
    ORR     r1, r2, #&80000           ; #524288 = bit 19
    ORR     r1, r2, #&770000          ; #7798784 = 119<<16
    ORR     r1, r2, #&6600000         ; #106954752 = 102<<20
    ORR     r1, r2, #&55000000        ; #1426063360 = 85<<24
    ORR     r1, r2, #&ee000000        ; #-301989888 = 238<<24
    ORR     r1, r2, #&80000001        ; #-2147483647
    ORR     r1, r2, #17, #30          ; #68  = 'D'

Test: Arithmetic pc-relative
    ADR     r1, &00008004
    ADR     r1, &00007ffc
    ADD     pc, pc, r2, LSL #2        ; Table dispatch index R2
    ADRNE   r0, &0000804c

Test: Load/Store indexed
    LDR     r1, [r2]
    STR     r1, [r2]
    LDRB    r1, [r2, #1]
    STRB    r1, [r2, #1]
    LDR     r1, [r2, #4]
    LDR     r1, [r2, #&228]
    LDR     r1, [r2, #-4]
    LDR     r1, [r2, #4]!
    LDR     r1, [r2, #-4]!
    LDR     r1, [r2, r3]
    LDR     r1, [r2, -r3]
    LDR     r1, [r2, r3, LSL #2]
    LDR     r1, [r2, r3, LSR #2]

Test: Load/Store post-indexed
    LDRB    r1, [r2], #1
    LDR     r1, [r2], #4
    LDR     r1, [r2], #-4
    LDR     r1, [r2], r3
    LDR     r1, [r2], -r3
    LDR     r1, [r2], r3, LSL #2
    LDR     r1, [r2], r3, LSR #2

Test: Load/Store pc-relative
    LDR     r1, &00008004
    LDR     r1, &00007ffc
    LDR     r1, [pc, r2]

Test: SWI
